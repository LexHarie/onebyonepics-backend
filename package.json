{
  "name": "onebyonepics-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "bun run src/main.ts",
    "start:dev": "bun run schema:validate && bun --watch src/main.ts",
    "build": "tsc -p tsconfig.json",
    "lint": "echo \"No lint setup\"",
    "migrate": "bun run scripts/migrate.ts",
    "migrate:up": "bun run scripts/migrate.ts up",
    "migrate:down": "bun run scripts/migrate.ts down",
    "migrate:status": "bun run scripts/migrate.ts status",
    "schema:validate": "bun run scripts/validate-schema.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.686.0",
    "@aws-sdk/s3-request-presigner": "^3.686.0",
    "@buiducnhat/nest-better-auth": "^1.1.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/multipart": "^9.3.0",
    "@google/genai": "^1.0.0",
    "@nestjs/bullmq": "^11.0.0",
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.0",
    "@nestjs/platform-fastify": "^11.0.0",
    "@nestjs/schedule": "^5.0.0",
    "bcrypt": "^5.1.1",
    "better-auth": "^1.4.7",
    "bullmq": "^5.0.0",
    "bun": "^1.3.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "fastify": "^5",
    "ioredis": "^5.4.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bun": "latest",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.1",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^3.0.13",
    "@types/pg": "^8.16.0",
    "typescript": "^5.6.3"
  }
}
